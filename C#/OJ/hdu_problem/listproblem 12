<script language="javascript">
function p(color,pid,solved,title,ac,sub)
{
	var item;
	var ratio;

	if(color)
		color = " bgcolor=#D7EBFF";
	else
		color = "";

	if(sub > 0)
	{
		ratio = (ac * 100 / sub + 0.005).toString();
		ratio = ratio.substring(0, ratio.indexOf('.') + 3);
	}
	else
		ratio = '0.00';

	document.write("<tr" + color + " align=center height=22>");
	if(solved == 0)
		document.write("<td></td>");
	else if(solved == 5)
		document.write("<td><img src=\"images/ac.gif\" width=16 height=16 alt=\"\n\tYou has solved this problem :-)\t\n\"></td>");
	else if(solved == 6)
		document.write("<td><img src=\"images/tried.gif\" width=16 height=16 alt=\"\n\tYou has tried this problem but hasn't solved it :-(\t\n\"></td>");
	document.write("<td>" + pid + "</td><td align=left><a href=\"showproblem.php?pid=" + pid + "\">&nbsp;" + title + "</a></td><td>" + ratio + "%(<a href=\"status.php?pid=" + pid + "&status=5\">" + ac + "</a>/<a href=\"status.php?pid=" + pid + "\">" + sub + "</a>)</td></tr>");
}
</script>
